# Configuración de la API para producción
REACT_APP_API_URL=https://tu-dominio.com/backend-api/api

# Configuración de la aplicación
REACT_APP_NAME=Sistema de Matrículas
REACT_APP_VERSION=1.0.0

# Configuración del entorno
NODE_ENV=production

# Configuración de timeouts (en milisegundos)
REACT_APP_API_TIMEOUT=10000

# Configuración de sesión (en minutos)
REACT_APP_SESSION_TIMEOUT=120

# Configuración de paginación
REACT_APP_DEFAULT_PAGE_SIZE=10
REACT_APP_MAX_PAGE_SIZE=50

# Configuración de UI
REACT_APP_THEME=default
REACT_APP_ANIMATIONS=true

# URLs de redirección
REACT_APP_LOGIN_URL=/
REACT_APP_DASHBOARD_URL=/dashboard

# Configuración de producción
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=error
